{
  "manifest_version": 2,
  "name": "Soundcloud Repost Remover",
  "author": "Austin Ray",
  "version": "0.0.1",
  "description": "Remove soundcloud reposts",
  "incognito": "split",
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "index.html",
    "default_title": "Click to reload url"
  },
  "permissions": [
    "activeTab"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
